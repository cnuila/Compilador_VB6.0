            }
            // fall through
          case 93: break;
          case 44:
            { return new Symbol(sym.STRINGTYPE,yyline,yycolumn,yytext());
            }
            // fall through
          case 94: break;
          case 45:
            { return new Symbol(sym.BOOLEANTYPE,yyline,yycolumn,yytext());
            }
            // fall through
          case 95: break;
          case 46:
